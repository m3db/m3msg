{
  "Linters": {
    "badtime": {
      "Command": "badtime",
      "Pattern": "PATH:LINE:COL:MESSAGE"
    },
    "deadcode": { 
      "Command": "deadcode  -tags integration"
    },
    "varcheck":  { 
      "Command": "varcheck  -tags integration" 
    },
    "megacheck": { 
      "Command": "megacheck -tags integration" 
    }
  },
  "Enable":
    [ "golint"
    , "deadcode"
    , "varcheck"
    , "structcheck"
    , "goconst"
    , "ineffassign"
    , "unconvert"
    , "misspell"
    , "unparam"
    , "megacheck"
    , "badtime" ],
  "Deadline": "3m",
  "EnableGC": true
}
